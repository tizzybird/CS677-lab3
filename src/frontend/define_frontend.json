{
    "testenv": 1,
    "withCache": 1,
    "timeout": 3,
    "topics": {
        "DS": "distributed systems",
        "GS": "graduate school"
    },
    "booklist": [{
        "number": 1,
        "title": "How to get a good grade in 677 in 20 minutes a day.",
        "topic": "DS"
    }, {
        "number": 2,
        "title": "RPCs for Dummies.",
        "topic": "DS"
    }, {
        "number": 3,
        "title": "Xen and the Art of Surviving Graduate School.",
        "topic": "GS"
    }, {
        "number": 4,
        "title": "Cooking for the Impatient Graduate Student.",
        "topic": "GS"
    }, {
        "number": 5,
        "title": "How to finish Project 3 on time",
        "topic": "DS"
    }, {
        "number": 6,
        "title": "Why theory classes are so hard",
        "topic": "GS"
    }, {
        "number": 7,
        "title": "Spring in the Pioneer Valley",
        "topic": "GS"
    }],
    "logConfig": {
        "version": 1,
        "formatters": {
            "default": {
                "format": "[%(asctime)s] %(levelname)s in %(module)s: %(message)s"
            },
            "time_diff": {
                "format": "%(message)s"
            },
            "heartbeat": {
                "format": "[%(asctime)s]: %(message)s"
            }
        },
        "handlers": {
            "wsgi": {
                "class": "logging.StreamHandler",
                "stream": "ext://flask.logging.wsgi_errors_stream",
                "formatter": "default"
            },
            "f_search": {
                "class": "logging.FileHandler",
                "filename": "frontend_search_time.log",
                "formatter": "time_diff"
            },
            "f_lookup": {
                "class": "logging.FileHandler",
                "filename": "frontend_lookup_time.log",
                "formatter": "time_diff"
            },
            "f_buy": {
                "class": "logging.FileHandler",
                "filename": "frontend_buy_time.log",
                "formatter": "time_diff"
            },
            "heartbeat": {
                "class": "logging.FileHandler",
                "filename": "frontend_heartbeat.log",
                "formatter": "heartbeat"
            }
        },
        "loggers": {
            "search": {
                "level": "INFO",
                "handlers": ["f_search"]
            },
            "lookup": {
                "level": "INFO",
                "handlers": ["f_lookup"]
            },
            "buy": {
                "level": "INFO",
                "handlers": ["f_buy"]
            },
            "heartbeat": {
                "level": "DEBUG",
                "handlers": ["heartbeat"]
            }
        },
        "root": {
            "level": "INFO",
            "handlers": ["wsgi"]
        }
    }
}